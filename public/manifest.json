{
  "manifest_version": 2,

  "name": "Michael Jordan chat extension",
  "description": "This extesion adds chat capability to every page",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html"
  },

  "background": {
    "page": "index.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
